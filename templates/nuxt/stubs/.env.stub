# Nuxt Environment Variables
NODE_ENV=development
NUXT_TELEMETRY_DISABLED=1

# Server
NITRO_HOST=0.0.0.0
NITRO_PORT=3000

# Database
DATABASE_URL=postgresql://postgres:password@db:5432/{{PROJECT_NAME}}

# Redis
REDIS_URL=redis://redis:6379

# Public runtime config (exposed to browser)
NUXT_PUBLIC_APP_URL=http://localhost:{{APP_PORT}}

# Private runtime config (server only)
# NUXT_API_SECRET=your-api-secret
